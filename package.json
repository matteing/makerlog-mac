{
  "name": "basic-electron-react-boilerplate",
  "version": "0.7.0",
  "description": "Minimal and modern react+electron+webpack boilerplate",
  "author": "Phillip Barbiero",
  "homepage": "https://github.com/pbarbiero/basic-electron-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbarbiero/basic-electron-react-boilerplate.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prod": "NODE_ENV=production webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "NODE_ENV=production webpack --config webpack.build.config.js",
    "package": "NODE_ENV=production webpack --config webpack.build.config.js",
    "postpackage": "NODE_ENV=production electron-packager ./ --out=./builds"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "electron": "^15.5.5",
    "electron-packager": "^14.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^7.0.27",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@reach/router": "^1.3.3",
    "animate.css": "^3.7.2",
    "axios": "^0.19.2",
    "babel-plugin-root-import": "^6.4.1",
    "babel-preset-mobx": "^2.0.0",
    "mobx": "^5.15.4",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.1.8",
    "postcss-loader": "^3.0.0",
    "tailwindcss": "^1.2.0"
  }
}
